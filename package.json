{
  "name": "chat",
  "version": "1.0.0",
  "description": "A simple Socket.io chat web app",
  "author": "Liang-Shih Lin",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production babel-node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-sass": "^4.12.0",
    "nuxt": "^2.10.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vue-socket.io-extended": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.4"
  }
